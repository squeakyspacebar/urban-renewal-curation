{
    "env": {
        "node": true,
        "es2020": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "window": true,
        "document": true
    },
    "parserOptions": {
        "sourceType": "module",
    },
    "rules": {
        "no-console": "off",
        "no-return-await": "error",
        "no-unused-vars": "warn",
        "no-prototype-builtins": "off"
    }
}
